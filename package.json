{
    "name": "angular-library-starter",
    "private": true,
    "version": "0.0.1",
    "description": "Build an Angular library compatible with AoT compilation and Tree shaking like an official package",
    "main": "./bundles/angular-library-starter.umd.js",
    "module": "./fesm5/angular-library-starter.js",
    "es2015": "./fesm2015/angular-library-starter.js",
    "esm5": "./esm5/angular-library-starter.js",
    "esm2015": "./esm2015/angular-library-starter.js",
    "fesm5": "./fesm5/angular-library-starter.js",
    "fesm2015": "./fesm2015/angular-library-starter.js",
    "typings": "./angular-library-starter.d.ts",
    "scripts": {
        "build": "node build.js",
        "test": "karma start",
        "test:watch": "karma start karma.conf.js --single-run false",
        "pack:lib": "npm run build && npm pack ./dist",
        "publish:lib": "npm run build && npm publish ./dist",
        "publish:lib:next": "npm run build && npm publish --tag next ./dist",
        "compodoc": "compodoc -p tsconfig.json",
        "compodoc:serve": "compodoc -s"
    },
    "author": "",
    "repository": {
        "type": "git",
        "url": "https://github.com/devjaime/core-lib-component.git"
    },
    "bugs": {
        "url": "https://github.com/devjaime/core-lib-component/issues"
    },
    "homepage": "https://github.com/devjaime/core-lib-component",
    "keywords": [
        "angular",
        "javascript",
        "typescript"
    ],
    "license": "MIT",
    "dependencies": {
        "tslib": "^1.9.0"
    },
    "peerDependencies": {
        "rxjs": ">= 6.4.0",
        "@angular/common": ">= 8.0.0",
        "@angular/core": ">= 8.0.0"
    },
    "devDependencies": {
        "@angular/animations": "8.0.0",
        "@angular/common": "8.0.0",
        "@angular/compiler": "8.0.0",
        "@angular/compiler-cli": "8.0.0",
        "@angular/core": "8.0.0",
        "@angular/platform-browser": "8.0.0",
        "@angular/platform-browser-dynamic": "8.0.0",
        "@angular/platform-server": "8.0.0",
        "@compodoc/compodoc": "1.1.9",
        "@types/jasmine": "3.3.13",
        "@types/node": "12.0.4",
        "chalk": "2.4.2",
        "codelyzer": "5.0.1",
        "core-js": "3.1.3",
        "istanbul-instrumenter-loader": "3.0.1",
        "jasmine-core": "3.4.0",
        "karma": "4.1.0",
        "karma-chrome-launcher": "2.2.0",
        "karma-coverage-istanbul-reporter": "2.0.5",
        "karma-jasmine": "2.0.1",
        "karma-sourcemap-loader": "0.3.7",
        "karma-spec-reporter": "0.0.32",
        "karma-webpack": "4.0.0-rc.3",
